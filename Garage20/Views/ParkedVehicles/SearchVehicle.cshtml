@model Garage20.Models.ParkedVehicle

@{
    ViewBag.Title = "SearchVehicle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Checka ut fordon</h2>

<div class="row">
    <div class="col-sm-12">
        @using (Html.BeginForm("Search", "ParkedVehicles", FormMethod.Get))
        {
            <p>Ange registreringsnummer för fordonet du vill checka ut</p>
            <ul class="nav navbar-nav">
                <li><input name="Search" class="form-control" placeholder="Ange registreringsnummer" type="text" /></li>
                <li><input type="submit" class="btn btn-default" value="Checka ut" /></li>
            </ul>
        }
 
    </div>

</div>
